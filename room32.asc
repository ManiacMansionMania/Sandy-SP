function oMikrowelle_AnyClick()
{
	if (any_click_move(66, 126, eDirectionUp))
	{
		if (UsedAction(eActWalkTo))
		{
		//	Wait(5);
		}
		else if (UsedAction(eActLookAt))
		{
			Wait(5);
			if ( oMikrowelle.Graphic == 638 )
			{
				player.Say("Das ist unsere neue Mikrowelle.");
			}
			else
			{
				player.Say("Leer.");
			}
		}
		else if (UsedAction(eActPickUp))
		{
			Wait(5);
			player.Say("Die brauche ich nicht.");
		}
		else if (UsedAction(eActOpen))
		{
			Wait(5);
			if ( oMikrowelle.Graphic == 638 )
			{
				PlaySound(85);
				oMikrowelle.Graphic=639;
			}
			else
			{
				player.Say("Ist doch schon offen.");
			}
		}
		else if (UsedAction(eActClose))
		{
			Wait(5);
			if ( oMikrowelle.Graphic == 638 )
			{
				player.Say("Ist doch schon zu.");
			}
			else
			{
				PlaySound(83);
				oMikrowelle.Graphic=638;
			} 
		}
		else if (UsedAction(eActPush))
		{
			Wait(5);
			player.Say("Warum sollte ich sie bewegen?");
		}
		else if (UsedAction(eActPull))
		{
			Wait(5);
			player.Say("Warum sollte ich sie bewegen?");
		}
		else if (UsedAction(eActUse))
		{
			Wait(5);
			if (GetGlobalInt(1)==0)
			{
				player.Say("Ich sollte sie erst ˆffnen.");
			}
			else if (GetGlobalInt(1)==1)
			{
				player.Say("Wenn nichts drin liegt, bringt das wenig.");
			}
		}
		else Unhandled();
	}
}

