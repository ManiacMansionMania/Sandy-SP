function hWasserhahn_AnyClick()
{
	if (any_click_move(270, 126, eDirectionUp))
	{
		if (UsedAction(eActWalkTo))
		{
		//	Wait(5);
		}
		else if (UsedAction(eActLookAt))
		{
			Wait(5);
			player.Say("Das Wasser daraus kann man sogar trinken.");
		}
		else if (UsedAction(eActPickUp))
		{
			Wait(5);
			player.Say("Damit ich immer was zu trinken habe, oder wie?");
		}
		else if (UsedAction(eActOpen))
		{
			Wait(5);
			if (GetGlobalInt(10)==0)
			{
				oWasser.Visible=true;
				PlayAmbientSound (1, 95, 150, 51, 131);
				SetGlobalInt(10, 1);
			}
			else if (GetGlobalInt(10)==1)
			{
				player.Say("Es fließt doch schon.");
			} 
		}
		else if (UsedAction(eActClose))
		{
			Wait(5);
			if (GetGlobalInt(10)==1)
			{
				oWasser.Visible=false;
				StopAmbientSound(1);
				SetGlobalInt(10, 0);
			}
			else if (GetGlobalInt(10)==0)
			{
				player.Say("Er ist schon geschlossen.");
			}
		}
		else if (UsedAction(eActPull))
		{
			Wait(5);
			player.Say("Damit ich immer was zu trinken habe, oder wie?");
		}
		else if (UsedAction(eActPush))
		{
			Wait(5);
			player.Say("Ich kann ihn nicht weiter rein drücken.");
		}
		else if (UsedAction(eActUse))
		{
			Wait(5);
			if (GetGlobalInt(10)==0)
			{
				oWasser.Visible=true;
				PlayAmbientSound (1, 95, 150, 51, 131);
				SetGlobalInt(10, 1);
			}
			else if (GetGlobalInt(10)==1)
			{
				player.Say("Es fließt doch schon.");
			}
		} 
		else Unhandled(); 
	}
}

