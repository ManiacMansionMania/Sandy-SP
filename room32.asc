function hFensterRechts_AnyClick()
{
	if (any_click_move(288, 123, eDirectionRight))
	{
		if (UsedAction(eActWalkTo))
		{
		//	Wait(5);
		}
		else if (UsedAction(eActLookAt))
		{
			Wait(5);
			player.Say("Nette Aussicht.");
		}
		else if (UsedAction(eActPickUp))
		{
			Wait(5);
			player.Say("Ich brauche kein Fenster.");
		}
		else if (UsedAction(eActOpen))
		{
			Wait(5);
			if ( oFenster.Visible == true )
			{
				player.Say("Ist doch schon offen.");
			}
			else
			{
				PlaySound(85);
				//hFensterRechts.Enabled=false;
				oFenster.Visible=true;
			}
		}
		else if (UsedAction(eActClose))
		{
			Wait(5);
			if ( oFenster.Visible == true )
			{
				PlaySound(85);
				oFenster.Visible = false;
				//hFensterRechts.Enabled = true;
			}
			else
			{
				player.Say("Ist doch schon zu.");
			}
		}
		else if (UsedAction(eActPush))
		{
			Wait(5);
			player.Say("Ist doch schon zu.");
		}
		else if (UsedAction(eActPull))
		{
			Wait(5);
			PlaySound(85);
			oFenster.Visible=true;
		}
		else if (UsedAction(eActUse))
		{
			Wait(5);
			if ( oFenster.Visible == true )
			{
				PlaySound(85);
				oFenster.Visible=false;
				//hFensterRechts.Enabled = true;
			}
			else
			{
				PlaySound(85);
				oFenster.Visible=true;
				//hFensterRechts.Enabled = false;
			}
		}
		else Unhandled();
	}
}


