function oSeitenschrank_AnyClick()
{
	if (any_click_move(34, 126, eDirectionUp))
	{
		if (UsedAction(eActWalkTo))
		{
			//	Wait(5);
		}
		else if (UsedAction(eActLookAt))
		{
			Wait(5);
			player.Say("Leer.");
		}
		else if (UsedAction(eActPickUp))
		{
			Wait(5);
			player.Say("Soll ich ihn da etwa rausreißen?");
